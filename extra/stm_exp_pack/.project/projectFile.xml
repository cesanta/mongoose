            <board Dvendor="STMicroelectronics:13" name="NUCLEO-F746ZG" vendor="STMicroelectronics"/>
            <project>
                <environment folder="nucleo-f746zg-cube-baremetal" load=".cproject" name="stm32cubeide"/>
                <environment folder="nucleo-f746zg-cube-baremetal" load="nucleo-f746zg-cube-baremetal.ioc" name="stm32cubemx"/>
            </project>
            <attributes>
