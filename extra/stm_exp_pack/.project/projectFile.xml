            <attributes>
                <category>Getting started</category>
                <category>Networking</category>
                <keyword>Web server</keyword>
                <keyword>HTTP</keyword>
                <keyword>MQTT</keyword>
                <keyword>SNTP</keyword>
                <keyword>network</keyword>
