                <keyword>Web server</keyword>
                <keyword>HTTP</keyword>
                <keyword>MQTT</keyword>
                <keyword>SNTP</keyword>
                <keyword>network</keyword>
            </attributes>
        </example>
        <example doc="README.md" folder="Examples/x/x/nucleo-f746zg-cube-freertos-lwip::Projects/NUCLEO-F746ZG/Examples/nucleo-f746zg-cube-freertos-lwip" name="nucleo-f746zg-cube-freertos-lwip" public="false" version="7.12.0">
